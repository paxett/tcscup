<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DsItemCardView">
        <attr name="android:title" format="string"/>
        <attr name="android:subtitle"/>
        <attr name="item_background" format="enum">
            <enum name="WHITE" value="0"/>
            <enum name="GREY" value="1"/>
        </attr>
        <attr name="item_isElevated" format="enum">
            <enum name="NO" value="0"/>
            <enum name="YES" value="1"/>
        </attr>
        <attr name="close_button_visibility" format="enum">
            <enum name="GONE" value="0"/>
            <enum name="VISIBLE" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="DsVerticalContainer">
    </declare-styleable>
</resources>